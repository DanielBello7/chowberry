{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/brevo": [
        "libs/brevo/src"
      ],
      "@app/brevo/*": [
        "libs/brevo/src/*"
      ],
      "@app/cloudinary": [
        "libs/cloudinary/src"
      ],
      "@app/cloudinary/*": [
        "libs/cloudinary/src/*"
      ],
      "@app/commons": [
        "libs/commons/src"
      ],
      "@app/commons/*": [
        "libs/commons/src/*"
      ],
      "@app/config": [
        "libs/config/src"
      ],
      "@app/constants": [
        "libs/constants/src"
      ],
      "@app/constants/*": [
        "libs/constants/src/*"
      ],
      "@app/drizzle": [
        "libs/drizzle/src"
      ],
      "@app/drizzle/*": [
        "libs/drizzle/src/*"
      ],
      "@app/enums": [
        "libs/enums/src"
      ],
      "@app/enums/*": [
        "libs/enums/src/*"
      ],
      "@app/exceptions": [
        "libs/exceptions/src"
      ],
      "@app/exceptions/*": [
        "libs/exceptions/src/*"
      ],
      "@app/flutterwave": [
        "libs/flutterwave/src"
      ],
      "@app/flutterwave/*": [
        "libs/flutterwave/src/*"
      ],
      "@app/rmq": [
        "libs/rmq/src"
      ],
      "@app/rmq/*": [
        "libs/rmq/src/*"
      ],
      "@app/types": [
        "types"
      ],
      "@app/types/*": [
        "types/*"
      ],
      "@app/winston": [
        "libs/winston/src"
      ],
      "@app/winston/*": [
        "libs/winston/src/*"
      ]
    }
  }
}